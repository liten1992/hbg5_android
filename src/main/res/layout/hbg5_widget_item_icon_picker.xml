<priv.liten.hbg5_widget.bin.layout.HBG5FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <priv.liten.hbg5_widget.bin.image.HBG5ImageView
        android:id="@+id/image_icon"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        app:v5BackgroundBorderSize="1dp"
        app:v5BackgroundBorderColor="@color/hbg5_silver"
        app:v5BackgroundRadius="fill"
        tools:v5BackgroundColor="@color/hbg5_gray"/>

</priv.liten.hbg5_widget.bin.layout.HBG5FrameLayout>